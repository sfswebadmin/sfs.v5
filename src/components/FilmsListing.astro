---
import  FilmListingArticle from './FilmListingArticle.astro';

const {films, siteData} = Astro.props;
---

<div class="row mt-3">
    <section class="col-lg-12">
        {
            films.map((film) => (
                <FilmListingArticle film={film} />
            ))
        }
    </section>
    <!--End of film listing section-->
</div>